                      \E5_X1.obj) EP

LOC  OBJ            LINE     SOURCE

                       1     ;  Convert an ASCII code available in external data memory location
                       2     ; into HEX equivalent and display in the Data field of the display.
                       3     
0000                   4     ORG 0000H                                       ; ORIGINATE
0000 0102              5     AJMP START                                      ; JUMP TO THE LABEL START
                       6     
0002                   7     START:
0002 905000            8             MOV DPTR, #5000H                ; DATA LOCATION
0005 E0                9             MOVX A, @DPTR                   ; GET THE DATA
                      10     
0006 C3               11             CLR C                                   ; CLEAR CARRY
0007 9430             12             SUBB A, #30H                    ; SUBTRACT (30H) INISTIALLY
0009 F9               13             MOV R1, A                               ; SAVE A COPY IN (R1)
                      14     
000A 940A             15             SUBB A, #0AH            ; CHECK FOR NUMBER OR ALPHA-NUMERIC CHARACTER
                      16     
                      17             ; IF NO CARRY ==> INPUT NUMBER IS < 3AH ==> [0-9]
000C 5003             18             JNC DISP
                      19     
                      20     ; IF CARRY ==> INPUT NUMBER IS >= 3AH ==> [A-F]
000E                  21     ALPH:
000E C3               22             CLR C                                   ; CLEAR CARRY AGAIN
000F 9407             23             SUBB A, #07H                    ; SUBTRACT AN ADDITIONAL (07H)
                      24     
0011                  25     DISP:
0011 7860             26             MOV R0, #60H                    ; LOCATION OF THE DISPLAY FIELD
0013 F6               27             MOV @R0, A                              ; UPDATE THE DATA TO THE LOCATION TO BE DIS
                             PLAYED
                      28     
0014                  29     HERE:
0014 80FE             30             SJMP HERE                               ; LOGICAL END
                      31             END
