LOC  OBJ            LINE     SOURCE

                       1     ; Convert a 2-digit hexadecimal number into BCD number.
                       2     
                       3     ; LIMITATIONS: ACCEPTS INPUT 2-DIGIT HEXADECIMAL IN DECIMAL RANGE
                       4     
0000                   5     ORG 0000H                               ; ORIGINATE
0000 0102              6     AJMP START                              ; JUMP TO THE LABEL START
                       7     
0002                   8     START:
0002 7892              9             MOV R0, #92H            ; INPUT 2-DIGIT HEXADECIMAL NUMBER, IN DECIMAL RANGE
0004 E8               10             MOV A, R0                       ; COPY TO THE ACCUMULATOR
                      11     
0005 540F             12             ANL A, #0FH                     ; MASK THE UPPER NIBBLE. RETURNS LOWER NIBBLE
0007 F9               13             MOV R1, A                       ; STORE LOWER NIBBLE IN (R1)
                      14     
0008 E8               15             MOV A, R0                       ; COPY TO THE ACCUMULATOR AGAIN SINCE INPUT IS DEST
                             ROYED
0009 54F0             16             ANL A, #0F0H            ; MASK THE LOWER NIBBLE. RETURNS UPPER NIBBLE
000B C4               17             SWAP A                          ; SWAP THE CURRENT LOWER AND UPPER NIBBLE IN ACCUMU
                             LATOR
000C FA               18             MOV R2, A                       ; STORE UPPER NIBBLE IN (R2)
                      19     
000D                  20     HERE:
000D 80FE             21             SJMP HERE                       ; LOGICAL END
                      22             END
