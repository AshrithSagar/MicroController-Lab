ASSEMBLER INVOKED BY: C:\Keil_v5\C51\BIN\A51.EXE E4_X2.ASM SET(SMALL) DEBUG PRINT(.\Listings\E4_X2.lst) OBJECT(.\Objects
                      \E4_X2.obj) EP

LOC  OBJ            LINE     SOURCE

                       1     ; Assuming R1:R0 registers as an 16 bit register rotate the 
                       2     ; content of this register left such that bit-7 of R0 becomes 
                       3     ; bit 0 of R1 and bit-7 of R1 becomes bit-0 of R0.
                       4     
0000                   5     ORG 0000H                               ; ORIGINATE
0000 0102              6     AJMP START                              ; JUMP TO THE LABEL START
                       7     
0002                   8     START:
0002 78FF              9             MOV R0, #0FFH           ; INPUT LOWER BYTE
0004 7900             10             MOV R1, #00H            ; INPUT HIGHER BYTE
0006 C3               11             CLR C                           ; CLEAR CARRY
                      12     
0007 E9               13             MOV A, R1                       ; GET HIGHER BYTE IN ACCUMULATOR
                      14             
                      15             ; MASK AND GET THE FIRST BIT OF THE HIGHER BYTE, AND STORE IN ACCUMULATOR
0008 5480             16             ANL A, #80H             ; (80H) IS (10000000B)
                      17     
                      18             ; IF ACCUMULATOR IS ZERO => BIT-7 OF HIGHER BYTE IS 0
000A 6009             19             JZ UP1
                      20     
                      21             ; IF ACCUMULATOR IS NON-ZERO => BIT-7 OF HIGHER BYTE IS 1
000C D3               22             SETB C                          ; SET THE CARRY BIT
                      23     
000D                  24     UP2:
000D E8               25             MOV A, R0                       ; GET LOWER BYTE IN ACCUMULATOR
000E 33               26             RLC A                           ; ROTATE LEFT ACCUMULATOR, WHICH HAS THE LOWER BYTE
000F FA               27             MOV R2, A                       ; STORE LOWER BYTE IN (R2)
                      28             
                      29             ; THE CARRY BIT IS NOW THE BIT-7 OF THE LOWER BYTE
                      30     
0010 E9               31             MOV A, R1                       ; GET HIGHER BYTE IN THE ACCUMULATOR AGAIN
0011 33               32             RLC A                           ; ROTATE LEFT ACCUMULATOR, WHICH HAS THE HIGHER BYTE
0012 FB               33             MOV R3, A                       ; STORE HIGHER BYTE IN (R3)
                      34     
0013 8003             35             SJMP HERE                       ; END
                      36     
0015                  37     UP1:
0015 C3               38             CLR C                           ; CLEAR CARRY
0016 80F5             39             SJMP UP2                        ; RETURN TO THE FLOW
                      40             
0018                  41     HERE:
0018 80FE             42             SJMP HERE                       ; LOGICAL END
                      43             END
